<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="500.0" prefWidth="383.0"
            style="-fx-background-color: #F7F7E8;"
            BorderPane.alignment="CENTER"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.sceneproject.fx_example.Controller">
    <children>
        <TextField fx:id="usernameInput"
                   layoutX="95.0" layoutY="212.0"
                   prefHeight="27.0" prefWidth="215.0"
                   promptText="Enter username...."
                   style="-fx-background-color: transparent; -fx-border-width: 0px 0px 2px 0px; -fx-border-color: #bfb051;">
            <font>
                <Font size="13.0"/>
            </font>
        </TextField>
        <PasswordField fx:id="passwordInput"
                       layoutX="95.0" layoutY="256.0"
                       prefHeight="27.0" prefWidth="215.0"
                       promptText="Enter password..."
                       style="-fx-background-color: transparent; -fx-border-color: #bfb051; -fx-border-width: 0px 0px 2px 0px;">
            <font>
                <Font size="13.0"/>
            </font>
        </PasswordField>
        <Button layoutX="100.0" layoutY="348.0" mnemonicParsing="false"
                onAction="#login" prefHeight="35.0" prefWidth="214.0"
                style="-fx-background-color: #763857;"
                text="Login" textFill="#eff0b6"/>
        <Hyperlink layoutX="140.0" layoutY="411.0" text="Zaboravili ste lozinku?"/>
        <Text fill="#763857" layoutX="95.0" layoutY="154.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="User Login" wrappingWidth="122.13671875">
            <font>
                <Font size="24.0"/>
            </font>
        </Text>
        <Label fx:id="loginMessageLabel" layoutX="133.0" layoutY="314.0" prefHeight="17.0" prefWidth="139.0"/>
    </children>
</AnchorPane>